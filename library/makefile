		gcc -o bin/l_freadInt.o src/lib_src/l_freadInt.c $(COMP_ARGS)

bin/l_readInt.o: src
		gcc -o bin/l_readInt.o src/lib_src/l_readInt.c $(COMP_ARGS)

bin/l_fnreadBool.o: src
		gcc -o bin/l_fnreadBool.o src/lib_src/l_fnreadBool.c $(COMP_ARGS)

bin/l_nreadBool.o: src
		gcc -o bin/l_nreadBool.o src/lib_src/l_nreadBool.c $(COMP_ARGS)

bin/l_fsnreadBool.o: src
		gcc -o bin/l_fsnreadBool.o src/lib_src/l_fsnreadBool.c $(COMP_ARGS)

