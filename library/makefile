bin/l_fnreadDouble.o: src
		gcc -o bin/l_fnreadDouble.o src/lib_src/l_fnreadDouble.c $(COMP_ARGS)

bin/l_fsnreadDouble.o: src
		gcc -o bin/l_fsnreadDouble.o src/lib_src/l_fsnreadDouble.c $(COMP_ARGS)

